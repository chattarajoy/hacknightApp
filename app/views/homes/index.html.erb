    <span class="label-text">Family History</span>
    <span class="nav-dot"></span>
  </label>
  <input id="input-6" type="text" placeholder="Y/N" required />
  <label for="input-6">
    <span class="label-text">Work Interfere</span>
    <span class="nav-dot"></span>
  </label>
  <input id="input-7" type="text" placeholder="1" required />
  <label for="input-7">
    <span class="label-text">No. of Employees</span>
    <span class="nav-dot"></span>
  </label><input id="input-8" type="text" placeholder="Y/N" required />
  <label for="input-8">
    <span class="label-text">Employee working remotely</span>
    <span class="nav-dot"></span>
  </label><input id="input-9" type="text" placeholder="Y/N" required />
  <label for="input-9">
    <span class="label-text">Tech Company</span>
    <span class="nav-dot"></span>
  </label><input id="input-10" type="text" placeholder="Y/N" required />
  <label for="input-10">
    <span class="label-text">Benefits</span>
    <span class="nav-dot"></span>
  </label><input id="input-11" type="text" placeholder="Y/N" required />
  <label for="input-11">
    <span class="label-text">Care options</span>
    <span class="nav-dot"></span>
  </label><input id="input-12" type="text" placeholder="Y/N" required />
  <label for="input-12">
    <span class="label-text">Wellness Program</span>
    <span class="nav-dot"></span>
  </label><input id="input-13" type="text" placeholder="Y/N" required />
