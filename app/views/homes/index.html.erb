    <span class="label-text">Seek Help</span>
    <span class="nav-dot"></span>
  </label><input id="input-14" type="text" placeholder="Y/N" required />
  <label for="input-14">
    <span class="label-text">Anonymity</span>
    <span class="nav-dot"></span>
  </label><input id="input-15" type="text" placeholder="Y/N" required />
  <label for="input-15">
    <span class="label-text">Leave</span>
    <span class="nav-dot"></span>
  </label><input id="input-16" type="text" placeholder="Y/N" required />
  <label for="input-16">
    <span class="label-text">Mental Health Consequence</span>
    <span class="nav-dot"></span>
  </label><input id="input-17" type="text" placeholder="Y/N" required />
  <label for="input-17">
    <span class="label-text">Physical Health Consequence</span>
    <span class="nav-dot"></span>
  </label><input id="input-18" type="text" placeholder="Y/N" required />
